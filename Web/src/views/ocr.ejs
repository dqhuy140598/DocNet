                CKEDITOR = editor
            } )
            .catch( error => {
                console.error( error );
            } );
      </script>   

      <script>
          function download(){
              var htmlText = CKEDITOR.getData();
              var object = {"html":htmlText}
              htmlText = htmlText.replace("&nbsp;",'')
              console.log(htmlText)
       
              $.ajax({  
                  type:'POST',
                  url: '/download',
                  data: htmlText,
                  // data: fd,
                  contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                  // cache:false,
                  // contentType: false,
                  processData: false,
                  // enctype: 'multipart/form-data',
                  success:function(data){
                      window.open("/download")
                  },
                  error: function(data){
                      console.log("error");
                      console.log(data);
                  }
              });
          } 
      
      </script>

      <script>
        function save(){
              var documentTitle = document.getElementById("title").value
              var content = window.content.result
              var image = window.content.filename

              var object = {documentTitle,content,image}
       
              $.ajax({  
                  type:'POST',
                  url: '/save',
                  data: JSON.stringify(object),
                  // data: fd,
                  contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                  // cache:false,
                  // contentType: false,
                  processData: false,
                  // enctype: 'multipart/form-data',
                  success:function(data){
                      console.log(data)
                      window.location.href = "/database"
                  },
                  error: function(data){
                      console.log("error");
                      console.log(data);
                  }
              });
        }


