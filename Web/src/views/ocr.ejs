   <script>
      function readUrl(input) {
            if (input.files && input.files[0]) {
              let reader = new FileReader();
              reader.onload = (e) => {
                let imgData = e.target.result;
                let imgName = input.files[0].name;
                input.setAttribute("data-title", imgName);
              }
              reader.readAsDataURL(input.files[0]);
            }
        }

        function modal(){
            $('.modal').modal('show');
          }

        $(document).ready(function (e) {
          $('#myform').on('submit',(function(e) {
              e.preventDefault();
              // var fd = new FormData();
              // var files = $('#file')[0].files[0];
              // fd.append('file',files);

              var form = $(this)[0];
              var formData = new FormData(form);
              console.log(formData)

              $.ajax({
                  type:'POST',
                  url: $(this).attr('action'),
                  data: formData,
                  // data: fd,
                  // contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                  // cache:false,
                  contentType: false,
                  processData: false,
                  // enctype: 'multipart/form-data',
                  success:function(data){
                      setTimeout(function () {
                        console.log(data);
                        console.log("success");
                        document.getElementById("img").src = "uploads/"+data.filename
                        $("#text").text("Image")
                        $('#ocr').css('display', 'none');
                        $('#result_image').css('display', '');
                        $('#myeditor').css('display', '');
                        $('.modal').modal('hide');
                        window.content = data
                        CKEDITOR.model.change( writer => {
                            const root = CKEDITOR.model.document.getRoot();
                            const insertPosition = CKEDITOR.model.document.selection.getFirstPosition();
                            for(var i=0;i<data.result.length;i++){
                                var p = writer.createElement( 'paragraph' );
                                writer.insertText( data.result[i],p );
                                writer.append( p, root );
                            }
                            
                        } );
                        var text = CKEDITOR.getData();
                        console.log(text)
                      }, 1000);
                  },
                  error: function(data){
                      console.log("error");
                      console.log(data);
                  }
              });
          }));

          $("#ImageBrowse").on("change", function() {
              $("#imageUploadForm").submit();
          });
      });
    </script>
      <script>
        let CKEDITOR;
        DecoupledEditor
            .create( document.querySelector( '#editor' ) )
